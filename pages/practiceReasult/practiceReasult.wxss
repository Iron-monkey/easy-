page{
  width: 100%;
  height: 100%;
  background-image: url(https://s3.ax1x.com/2020/12/23/rsjB4O.jpg);
  background-size: 100% 100%;
}
.formulaPreview {
  position: relative;
  height: 80rpx;
  padding: 0 40rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #eeeeee;
  font-size: 40rpx;
  background-color: rgba(255, 204, 204, 0.5);
  
}
.formulaPreview image {
  position: absolute;
  top: 18rpx;
  left: 260rpx;
  width: 48rpx;
}
text {
  float: right;
  color: rgb(0, 128, 122);
}
.choose {
  margin-top: 40rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.choose navigator {
  width: 33.3%;
  height: 80rpx;
  border-radius: 30rpx;
  background-color: #ff9999;
  text-align: center;
  line-height: 80rpx;
  color: white;
}