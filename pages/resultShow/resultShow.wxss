page {
  width: 100%;
  height: 100%;
  background-image: url(https://s3.ax1x.com/2020/12/12/rEOgxA.jpg);
  background-size: 100% 100%;
}
.img_preview {
  margin: 30rpx auto;
  width: 80%;
  height: 350rpx;
  border: 1px dashed #1296db;
  border-radius: 20rpx;
}
.img_preview image {
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
.btn {
  display: flex;
  margin: 30rpx auto;
  justify-content: space-around;
  width: 80%;
  height: 80rpx;
}
.btn_1 {
  width: 200rpx;
  height: 80rpx;
  color: white;
  line-height: 80rpx;
  text-align: center;
  border-radius: 20rpx;
  background-color: #99CCFF;
}
.resultShow {
  margin: 30rpx auto;
  width: 500rpx;
  height: 300rpx;
  border: 1px dashed gold;
  background-color: #fff;
  text-align: center;
}